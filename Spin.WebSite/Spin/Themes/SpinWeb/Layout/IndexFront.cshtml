    <link href="/lib/intl-tel-input/build/css/intlTelInput.min.css" rel="stylesheet" type="text/css" />


    <!--General css-->
    <link href="/Spin/Themes/SpinWeb/Resource/css/BaseCMSSpin.css" rel="stylesheet" type="text/css" />
    <link href="/Spin/Themes/SpinWeb/Resource/css/BaseCMSSpin-media.css" rel="stylesheet" type="text/css" />


    <!--Additional CSS-->
    @SpinViewPage.Styles.Render()


    <!--Icon-->
    <link rel="icon" href="" type="image/x-icon">
    <link rel="shortcut icon" href="" type="image/x-icon">


</head>

<body>
